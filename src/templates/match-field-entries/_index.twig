{% extends '_layouts/elementindex' %}
{% set title = 'Match field entries'|t('cockpit') %}
{% set elementType = 'craftpulse\\cockpit\\elements\\MatchFieldEntry' %}
{% set selectedTab = 'match-field-entries' %}
{% set selectedSubnavItem = 'match-field-entries' %}
{% set canHaveDrafts = true %}

{% block head %}
    {{ parent() }}
    {% set tagOptions = {
        'depends': [
            'craftpulse\\cockpit\\assetbundles\\cockpit\\CockpitCpAsset',
            'craftpulse\\cockpit\\assetbundles\\cockpit\\MatchFieldAsset'
        ],
    } %}

    {{ craft.cockpit.register('src/js/Cockpit.js', false, tagOptions, tagOptions) }}
    {{ craft.cockpit.register('src/js/MatchFields.js', false, tagOptions, tagOptions) }}
{% endblock %}

{% if matchFieldTypeHandle is defined %}
    {% js %}
        window.defaultMatchFieldTypeHandle = '{{ matchFieldTypeHandle }}';
    {% endjs %}
{% endif %}
